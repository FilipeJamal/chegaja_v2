# =========================================
# ChegaJá v2.5 — Configuração (EXEMPLO)
#
# Copia este ficheiro para `.env` e preenche.
# NÃO coloques aqui chaves secretas em builds de produção.
# =========================================

# -------- App (Flutter) --------
# Web Push (FCM)
FCM_VAPID_KEY=

# Stripe (PUBLICÁVEL)
STRIPE_PUBLISHABLE_KEY=pk_test_XXXXXXXXXXXXXXXXXXXXXXXX

# Ativar emuladores Firebase no DEV
USE_FIREBASE_EMULATORS=true
FIREBASE_EMULATOR_HOST=localhost

# Comissão da plataforma (fallback se Remote Config não estiver configurado)
DEFAULT_COMMISSION_RATE=0.15

# URL base do teu domínio (para App Links / Universal Links)
APP_BASE_URL=https://app.chegaja.exemplo
